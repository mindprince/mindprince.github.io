---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

  <title><![CDATA[{{ site.name | cdata_escape }}]]></title>
  <link href="{{ site.url }}/atom.xml" rel="self" type="application/atom+xml"/>
  <link href="{{ site.url }}/"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>{{ site.url }}/</id>
  <rights>Copyright (c) {{ site.time | date: '%Y' }} - Rohit Agarwal</rights>
  <author>
    <name>Rohit Agarwal</name>
  </author>

  {% for post in site.posts limit: 20 %}
  <entry>
    <title type="html"><![CDATA[{{ post.title | cdata_escape }}]]></title>
    <link href="{{ site.url }}{{ post.url }}"/>
    <published>{{ post.date | date_to_xmlschema }}</published>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <id>{{ site.url }}{{ post.id }}</id>
    <content type="html"><![CDATA[{{ post.content | cdata_escape }}]]></content>
    <author>
      <name>Rohit Agarwal</name>
    </author>
  </entry>
  {% endfor %}
</feed>

